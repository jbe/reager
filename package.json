{
  "name": "@josteinbe/reager",
  "version": "0.1.1",
  "private": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jbe/reager.git"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "babel": {
    "presets": [
      "@babel/react"
    ]
  },
  "dependencies": {
    "@babel/cli": "^7.5.5",
    "cross-env": "^5.2.0",
    "md5": "^2.2.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-scripts": "3.1.1",
    "rimraf": "^3.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build-app": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build-package": "rimraf dist && mkdir dist && cross-env NODE_ENV=production npx babel src/reager --out-dir dist --copy-files",
    "prepublish": "yarn run build-package"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "peerDependencies": {
    "react-router": "^5.0.1"
  }
}
